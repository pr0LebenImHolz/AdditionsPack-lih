{
    "multipart": [
        {   "apply": { "model": "additions:lih-portal_pane_4_post" }},
        {   "when": { "north": true },
            "apply": { "model": "additions:lih-portal_pane_4_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "additions:lih-portal_pane_4_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "additions:lih-portal_pane_4_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "additions:lih-portal_pane_4_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "additions:lih-portal_pane_4_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "additions:lih-portal_pane_4_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "additions:lih-portal_pane_4_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "additions:lih-portal_pane_4_noside", "y": 270 }
        }
    ]
}
